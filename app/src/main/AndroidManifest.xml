<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wizarpos.mdbtest">

    <uses-permission android:name="android.permission.CLOUDPOS_SERIAL" />
    <uses-permission android:name="android.permission.CLOUDPOS_MCUCTL" />
    <uses-permission android:name="android.permission.CLOUDPOS_INNER_GPIO" />
    <!-- MSR -->
    <uses-permission android:name="android.permission.CLOUDPOS_MSR" />
    <!-- Contactless card-->
    <uses-permission android:name="android.permission.CLOUDPOS_CONTACTLESS_CARD" />
    <!-- SmartCard-->
    <uses-permission android:name="android.permission.CLOUDPOS_SMARTCARD" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:networkSecurityConfig="@xml/network_security_config"
        android:icon="@drawable/icon_down"
        android:label="@string/app_name"
        android:theme="@style/Theme.MDBTransaction">
        <activity android:name="com.wizarpos.mdbtest.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>